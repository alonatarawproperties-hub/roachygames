{
  "expo": {
    "name": "Roachy Games",
    "slug": "roachy-games",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "roachy-games",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#120a05",
    "newArchEnabled": false,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b771b4a4-642a-40c9-98e4-f7af33384b5d",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cryptocreatures.app",
      "buildNumber": "122",
      "backgroundColor": "#120a05",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Roachy Hunt needs your location to find Roachies near you.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Roachy Hunt needs your location to spawn Roachies around you even when the app is in the background.",
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "phantom",
          "solflare",
          "backpack",
          "metamask",
          "trust",
          "rainbow"
        ],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.433710630203-d0o848q0ir44po4l06n3986jr3v05eav"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#120a05",
        "foregroundImage": "./assets/images/icon.png"
      },
      "package": "com.cryptocreatures.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "roachy.games",
              "pathPrefix": "/app"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-system-ui",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-native.png",
          "resizeMode": "cover",
          "backgroundColor": "#120a05",
          "dark": {
            "backgroundColor": "#120a05"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Roachy Hunt to use your location to find Roachies near you."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Roachy Hunt to use your camera to catch Roachies in AR mode."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "b771b4a4-642a-40c9-98e4-f7af33384b5d"
      },
      "walletConnectProjectId": "${EXPO_PUBLIC_WALLETCONNECT_PROJECT_ID}"
    },
    "owner": "roachygames"
  }
}
